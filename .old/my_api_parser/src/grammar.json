{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "api_doc",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "doc_comment"
      }
    },
    "doc_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "/**"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "function_tag"
              },
              {
                "type": "SYMBOL",
                "name": "class_tag"
              },
              {
                "type": "SYMBOL",
                "name": "param_tag"
              },
              {
                "type": "SYMBOL",
                "name": "property_tag"
              },
              {
                "type": "SYMBOL",
                "name": "returns_tag"
              },
              {
                "type": "SYMBOL",
                "name": "doc_text"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "*/"
        }
      ]
    },
    "function_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@function"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "class_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@class"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "param_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@param"
        },
        {
          "type": "SYMBOL",
          "name": "type"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "-"
        },
        {
          "type": "SYMBOL",
          "name": "description"
        }
      ]
    },
    "property_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@property"
        },
        {
          "type": "SYMBOL",
          "name": "type"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "-"
        },
        {
          "type": "SYMBOL",
          "name": "description"
        }
      ]
    },
    "returns_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@returns"
        },
        {
          "type": "SYMBOL",
          "name": "type"
        },
        {
          "type": "SYMBOL",
          "name": "description"
        }
      ]
    },
    "doc_text": {
      "type": "PATTERN",
      "value": "[^@*/\\s]([^\\n]*)"
    },
    "type": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "PATTERN",
          "value": "[^{}\\n]+"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "description": {
      "type": "PATTERN",
      "value": "[^\\n]*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "PATTERN",
      "value": "\\*+!?"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}