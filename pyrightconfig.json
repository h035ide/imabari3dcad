{
  "exclude": [
    "**/__pycache__",
    "**/chroma_db_store",
    "**/vector_store",
    "**/langflow_env",
    "**/evoship",
    "**/htmlcov",
    "**/node_modules",
    "**/.git",
    "**/venv",
    "**/env",
    "**/.venv",
    "**/site-packages",
    "**/dist",
    "**/build",
    "**/*.egg-info",
    "**/tests/__pycache__",
    "**/code_parser/__pycache__",
    "**/code_generator/__pycache__",
    "**/doc_parser/__pycache__",
    "**/db_integration/__pycache__",
    "**/suyama/__pycache__",
    "**/tests/__pycache__",
    "**/.uv",
    "**/mycode",
    "**/suyama",
    "**/*.stl",
    "**/*.dxf",
    "**/*.evomdl",
    "**/*.PNG",
    "**/*.png",
    "**/*.html",
    "**/*.css",
    "**/*.js"
  ],
  "ignore": [
    "**/chroma_db_store/**",
    "**/vector_store/**",
    "**/langflow_env/**",
    "**/evoship/**",
    "**/htmlcov/**",
    "**/.uv/**",
    "**/mycode/**",
    "**/suyama/**"
  ],
  "typeCheckingMode": "standard",
  "useLibraryCodeForTypes": true,
  "autoImportCompletions": true,
  "pythonVersion": "3.10",
  "pythonPlatform": "Windows",
  "reportMissingImports": "warning",
  "reportUnusedImport": "information",
  "reportUnusedVariable": "warning",
  "reportGeneralTypeIssues": "warning",
  "include": [
    "code_generator/**",
    "code_parser/**",
    "doc_parser/**",
    "db_integration/**",
    "graphrag_gpt/**",
    "tests/**",
    "*.py"
  ],
  "executionEnvironments": [
    {
      "root": ".",
      "pythonVersion": "3.10",
      "pythonPlatform": "Windows",
      "extraPaths": [
        "code_generator",
        "code_parser",
        "doc_parser",
        "db_integration",
        "graphrag_gpt"
      ]
    }
  ],
  "indexing": false
}
